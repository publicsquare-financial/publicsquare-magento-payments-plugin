{
  "name": "publicsquare-financial/magento-payments-plugin",
  "description": "The Magento plugin provides a one-click install into PublicSquare's payment gateway for credit and debit card processing.",
  "type": "composer-plugin",
  "autoload": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "codeception/codeception": "^5.1",
    "codeception/module-asserts": "*",
    "codeception/module-webdriver": "^4.0",
    "codeception/module-db": "^3.1",
    "codeception/module-phpbrowser": "^3.0",
    "codeception/module-cli": "^2.0",
    "codeception/module-rest": "^3.4"
  },
  "config": {
    "allow-plugins": {
      "magento/composer-dependency-version-audit-plugin": true
    }
  }
}
